{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "VERCEL": "1"
  },
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}